a = floors.replace(/\(/g,'a ');			
			b = a.replace(/\)/g,'b ');
			i = b;
					
			aTotal = ((i.match(new RegExp("a", "g")) || []).length);			
			bTotal = ((i.match(new RegExp("b", "g")) || []).length);

			var str = i;
			var result = str.split(" ");
		  var currentFloor = 0;

			for (var x = 0; x < result.length; x++){
        var currentInput = result[x];
        if (currentInput == "a"){
          var currentFloor = currentFloor + 1;
        }
        else {
          var currentFloor = currentFloor - 1;
        }
        
        if (currentFloor == -1) {
		document.write("<br/> <br/>" + "The Answer is:");
          document.write(x + 1);
          break;
        }
        
        document.write(currentFloor + " ");
      }
			
			console.log("A total = " + aTotal);
			console.log("B total = " + bTotal);
			
			console.log("aTotal - bTotal...");

			console.log("Floors = " + aTotal - bTotal);
			
			console.log("Floors = " + aTotal - bTotal);
